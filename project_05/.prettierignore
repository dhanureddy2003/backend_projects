*.log
*.csv
*.svg

# Ignore JavaScript files in a specific directory:
src/vendor/*.js

# Ignore all HTML files:
*.html

# Ignore files generated by the OS or editors:
.DS_Store
Thumbs.db
.vscode
.idea

# Ignore specific files:
src/ignored-file.js
